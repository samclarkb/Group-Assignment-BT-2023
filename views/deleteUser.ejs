<div class="deleteModal">
    <%- include('partials/header', { image:
	userinfo[0].Profilepic.data, username: userinfo[0].Username }) %>
	<form class="deleteForm" action="delUser<%= data[0]._id %>" method="POST">
		<h2>Are you sure you want to delete this User?</h2>
		<div>
			<label for="<%= data[0]._id %>">Yes</label>
			<input id="<%= data[0]._id %>" name="deleteUserOne" type="submit" value="<%= data[0]._id %>" />
			<a href="/usersDev">No</a>
		</div>
	</form>

	<div class="userCard">
		<img src="/images/<%= data[0].Profilepic.data %>" alt="profilePic" />
		<div>
			<p><%= data[0].Username %></p>
			<p><%= data[0].Email %></p>
		</div>
	</div>

	<%- include('partials/navigation') %>
</div>
